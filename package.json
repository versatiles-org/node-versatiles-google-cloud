{
  "name": "@versatiles/google-cloud",
  "version": "1.0.1",
  "description": "A server for VersaTiles in Google Cloud Run",
  "bin": {
    "versatiles-google-cloud": "dist/index.js"
  },
  "prefix": "./",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "static"
  ],
  "eslintConfig": {
    "root": true
  },
  "scripts": {
    "build:node": "rm -rf dist && tsc -p tsconfig.build.json && chmod +x dist/index.js",
    "build": "npm run build:node && npm run doc",
    "check": "npm run format:check && npm run lint && npm run test && npm run build",
    "coverage": "vitest run --coverage",
    "dev": "tsx watch src/index.ts",
    "doc:command": "vrt doc-command versatiles-google-cloud | vrt doc-insert README.md '## Options'",
    "doc:graph": "vrt deps-graph | vrt doc-insert README.md '## Dependency Graph'",
    "doc": "npm run doc:command && npm run doc:graph",
    "format": "prettier --write 'src/**/*.ts' '*.json' '*.js'",
    "format:check": "prettier --check 'src/**/*.ts' '*.json' '*.js'",
    "lint": "eslint . --color",
    "prepack": "npm run build",
    "release": "vrt release-npm",
    "start": "tsx src/index.ts",
    "test": "vitest run",
    "upgrade": "vrt deps-upgrade"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/versatiles-org/node-versatiles-google-cloud.git"
  },
  "homepage": "https://github.com/versatiles-org/node-versatiles-google-cloud#readme",
  "type": "module",
  "author": "yetzt <node@yetzt.me>, Michael Kreil <versatiles@michael-kreil.de>",
  "license": "Unlicense",
  "engines": {
    "node": ">= 20"
  },
  "keywords": [
    "cloudtiles",
    "geo",
    "gis",
    "map",
    "maps",
    "mapserver",
    "maptiles",
    "mvt",
    "opencloudtiles",
    "osm",
    "pbf",
    "tile",
    "tiles",
    "tileserver",
    "vectortiles",
    "versatiles"
  ],
  "overrides": {
    "fast-xml-parser": "5.3.4"
  },
  "dependencies": {
    "@eslint/js": "^10.0.1",
    "@google-cloud/storage": "^7.19.0",
    "@versatiles/container": "^1.4.0",
    "@versatiles/style": "^5.9.4",
    "c12": "^3.3.3",
    "commander": "^14.0.3",
    "express": "^5.2.1",
    "mrmime": "^2.0.1",
    "path-to-regexp": "^6.3.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.3.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@versatiles/release-tool": "^2.7.2",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18"
  }
}
